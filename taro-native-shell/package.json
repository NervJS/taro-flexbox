{
  "name": "taro-native-shell",
  "version": "0.0.1",
  "private": true,
  "description": "Taro native shell",
  "scripts": {
    "preinstall": "yarn cache clean",
    "postinstall": "yarn pod",
    "pod": "cd ios && pod install",
    "install-third-party": "cd node_modules/react-native/scripts && ./ios-install-third-party.sh",
    "glog-config": "cd node_modules/react-native/third-party/glog-0.3.5 && ./configure",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro-native-shell.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "expo-brightness": "^6.0.0",
    "expo-image-picker": "^6.0.0",
    "expo-location": "^6.0.0",
    "expo-sensors": "^6.0.0",
    "react": "16.8.6",
    "react-native": "0.62.3",
    "react-native-canvas": "^0.1.32",
    "react-native-image-crop-picker": "^0.23.1",
    "react-native-image-picker": "^0.28.1",
    "react-native-maps": "^0.25.0",
    "react-native-unimodules": "^0.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-jest": "^24.5.0",
    "conventional-changelog-cli": "^2.0.12",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
