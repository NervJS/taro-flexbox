{
  "name": "taro-flexbox",
  "version": "1.0.0",
  "private": true,
  "description": "基于 taro 的 FlexBox 跨端测试",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev": "npm-run-all dev:*",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "deployed": "cross-env OUTPUT_BUILD=h5 taro build --type h5"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/components": "1.3.19",
    "@tarojs/components-qa": "^1.3.19",
    "@tarojs/components-rn": "^1.3.19",
    "@tarojs/router": "1.3.19",
    "@tarojs/taro": "1.3.19",
    "@tarojs/taro-alipay": "1.3.19",
    "@tarojs/taro-h5": "1.3.19",
    "@tarojs/taro-qq": "1.3.19",
    "@tarojs/taro-quickapp": "1.3.19",
    "@tarojs/taro-redux-rn": "^1.3.19",
    "@tarojs/taro-rn": "^1.3.19",
    "@tarojs/taro-router-rn": "^1.3.19",
    "@tarojs/taro-swan": "1.3.19",
    "@tarojs/taro-tt": "1.3.19",
    "@tarojs/taro-weapp": "1.3.19",
    "classnames": "^2.2.6",
    "nerv-devtools": "^1.4.4",
    "nervjs": "^1.4.4",
    "react": "16.8.6",
    "react-native": "0.62.3",
    "redux": "^4.0.0",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@tarojs/plugin-babel": "1.3.19",
    "@tarojs/plugin-csso": "1.3.19",
    "@tarojs/plugin-sass": "1.3.19",
    "@tarojs/plugin-uglifyjs": "1.3.19",
    "@tarojs/webpack-runner": "1.3.19",
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-taro": "1.3.19",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-taro": "1.3.19",
    "npm-run-all": "^4.1.5",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "1.3.19",
    "stylelint-taro-rn": "1.3.19",
    "typescript": "^3.0.1"
  },
  "resolutions": {
    "react-native-unimodules": "0.5.2",
    "expo-av": "^6.0.0",
    "expo-brightness": "^6.0.0",
    "expo-file-system": "^6.0.0",
    "expo-image-picker": "^6.0.0",
    "expo-keep-awake": "^6.0.0",
    "expo-location": "^6.0.0",
    "expo-sensors": "^6.0.0"
  }
}
