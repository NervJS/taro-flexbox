{
	"name": "taro-flexbox",
	"version": "1.0.0",
	"private": true,
	"description": "基于 taro 的 FlexBox 跨端测试",
	"templateInfo": {
		"name": "default",
		"typescript": true,
		"css": "sass"
	},
	"scripts": {
		"build": "npm-run-all build:*",
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:quickapp": "taro build --type quickapp",
		"dev": "npm-run-all dev:*",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch",
		"deployed": "cross-env OUTPUT_BUILD=h5 taro build --type h5"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@tarojs/components": "2.2.6",
		"@tarojs/components-qa": "2.2.6",
		"@tarojs/components-rn": "2.2.6",
		"@tarojs/rn-runner": "^2.2.6",
		"@tarojs/router": "2.2.6",
		"@tarojs/taro": "2.2.6",
		"@tarojs/taro-alipay": "2.2.6",
		"@tarojs/taro-h5": "2.2.6",
		"@tarojs/taro-qq": "2.2.6",
		"@tarojs/taro-quickapp": "2.2.6",
		"@tarojs/taro-redux-rn": "2.2.6",
		"@tarojs/taro-rn": "2.2.6",
		"@tarojs/taro-router-rn": "2.2.6",
		"@tarojs/taro-swan": "2.2.6",
		"@tarojs/taro-tt": "2.2.6",
		"@tarojs/taro-weapp": "2.2.6",
		"classnames": "^2.2.6",
		"nerv-devtools": "^1.5.6",
		"nervjs": "^1.5.6",
		"react": "16.8.6",
		"react-native": "0.59.10",
		"redux": "^4.0.0",
		"tslib": "^1.8.0"
	},
	"devDependencies": {
		"@tarojs/plugin-babel": "2.2.6",
		"@tarojs/plugin-csso": "2.2.6",
		"@tarojs/plugin-sass": "2.2.6",
		"@tarojs/plugin-uglifyjs": "2.2.6",
		"@tarojs/webpack-runner": "2.2.6",
		"@types/react": "^16.4.6",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/parser": "^1.6.0",
		"babel-eslint": "^8.2.3",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-jsx-stylesheet": "^0.6.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"cross-env": "^5.2.0",
		"eslint": "^5.16.0",
		"eslint-config-taro": "2.2.6",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-taro": "2.2.6",
		"npm-run-all": "^4.1.5",
		"stylelint": "9.3.0",
		"stylelint-config-taro-rn": "2.2.6",
		"stylelint-taro-rn": "2.2.6",
		"typescript": "^3.0.1"
	},
	"resolutions": {
		"react-native-unimodules": "0.5.2",
		"expo-av": "^6.0.0",
		"expo-brightness": "^6.0.0",
		"expo-file-system": "^6.0.0",
		"expo-image-picker": "^6.0.0",
